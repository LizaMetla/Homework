package hw;
import java.util.Random;

public class zadan1 {
	
	public static void main(String[] args) {
		
		Random rand = new Random();
		
		int m = 10 + rand.nextInt(5);
		
		long num = 1;
		
		for (int i=1; i<=m; i++) {
			num = num*i;		
		}
		System.out.println("Факториалом " + m + " является: " + 
		"\n" + num);
		
		System.out.println();
		

}
}
